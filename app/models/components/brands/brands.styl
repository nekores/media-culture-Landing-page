.brands
	overflow hidden
	li
		text-align center
		padding-top 30px
		padding-bottom 30px
		display-inline block
		position relative
		// overflow hidden
		&::before
			content ''
			position absolute
			height 100%
			top 0px
			left -1px
			border-left 1px dashed
			color #999
		&::after
			content	 ' '
			position absolute
			height 0
			width 100%
			top auto
			left 0
			bottom -1px
			color #999
			border-bottom 1px dashed
	.brands-company
		a
			img
				opacity 0.7
				&:hover
					opacity 1
.col-md-5th
	width 20% !important
	float left