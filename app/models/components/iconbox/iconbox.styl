.iconbox-7
	cursor pointer
	.iconbox-7-inner
		position relative
		transition 0.5s all
		border 1px solid #FFCB27
		.ib-icon-7
			.icon-flat-lg
				display block
				position absolute
				background-color #333
				top -20px
				border 1px solid #FFCB27
				color white
				left -20px
		&:hover
			background-color #FFCB27
			color white
			.text
				.title
					color white
				p
					color white
		.ib-button-7
			background-color #333
			text-transform  uppercase
			position absolute
			bottom -43px
			right -16px
			widht 200px
			.btn-default
				background-color #333
				color primary
				border 1px solid #FFCB27
				font-weight 400
				font-size 16px
	.title
		color #333
	.description
		p
			color #333
			
.iconbox-8
	cursor pointer
	.iconbox-8-inner
		.ib-button-8
			.btn
				padding-top 8px
			.btn-primary
				font-weight 100px !important
				font-size 12px
				padding-right 10px
				letter-spacing 0.9px
				border-radius 0px
				text-transform uppercase !important
				box-shadow .6px .6px .8px .1px black
				text-shadow 1px 0px 1px black
				border-color primary
				background-color primary
				span
					top -3px
					left 100px
					box-shadow .6px .6px .8px .1px black
					padding 11px
					background-color darken(primary, 20%)
					font-size 15px !important
				&:hover, &:focus, &:active
					color white
					background-color darken(primary, 20%)
					span
						background-color lighten(primary, 10%)
				&:focus
					outline none
		.text
			.title
				font-size 16px !important
				letter-spacing 0.9px !important
				color darken(white, 50%)
			.title-slogan
				p
					text-transform uppercase
					letter-spacing 0.7px
					color black
					font-weight 400
					font-size 12px !important
			.description
				p
					letter-spacing 0.9px
					font-size 16px !important
					color black
					font-weight 200
					
