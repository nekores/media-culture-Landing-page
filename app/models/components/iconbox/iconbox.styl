.iconbox-1
	cursor pointer
	.iconbox-1-inner
		border 1px solid primary
		position relative
		.ib-icon-1
			.icon-lg
				margin auto
				display block
				position absolute
				top -1px
				left -1px
		.ib-button-1
			margin-top 20px
			margin-bottom 40px
			a
				color primary
				&:hover
					color #333
		&:hover
			.icon-lg
				border-left-color white
				border-top-color white
				transition 0.5s all
				border-radius 0px 0px 30px 0px
				background-color transparent
				.fa
					color primary

.iconbox-2
	cursor pointer
	.iconbox-2-inner
		position relative
		border 1px solid primary
		.ib-icon-2
			.icon-lg
				height 35px
				padding 0
				display block
				position absolute
				border-color transparent
				background-color white
				top -35px
				font-size 30px
				left 20px
		.ib-button-2
			a
				margin auto
				color darken(primary, 50%)
				&:hover
					color primary
				&:focus
					color primary
			a.active, &:visited
				color primary
		&:hover
			.icon-lg	
				transition 0.5s all
				left 40%
				background-color white
				.fa
					color primary

