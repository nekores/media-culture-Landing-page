


mixin countdown1()
	section()&attributes(attributes)
		.container
			.row
				.col-md-10.col-md-offset-1
					div(class="timer border-right")
						div(class="col-md-3 text-center")
							div(class="time-box days")
								div(class="tick")
									span.clr-white {dn}
						
								hr(style="border-color:#fff !important")
								div(class="title")
									p.clr-white DAYS
						div(class="col-md-3 text-center")
							div(class="time-box days")
								div(class="tick")
									span.clr-white {hn}
						
								hr(style="border-color:#fff !important")
								div(class="title")
									p.clr-white HOURS
						div(class="col-md-3 text-center")
							div(class="time-box days")
								div(class="tick")
									span.clr-white {mn}
						
								hr(style="border-color:#fff !important")
								div(class="title")
									p.clr-white MINS
						div(class="col-md-3 text-center")
							div(class="time-box days")
								div(class="tick")
									span.clr-white {sn}
						
								hr(style="border-color:#fff !important")
								div(class="title")
									p.clr-white SECS
					


