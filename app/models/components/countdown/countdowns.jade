mixin countdown7()
	section()&attributes(attributes)#countdown7
		.container
			.row
				.col-md-12
					div(class="timer no-border")
						div(class="col-md-10 col-sm-10 text-center nav-m30px")
							div(class="time-box days")
								div(class="tick")
									span.clr-white.fs60px.font-fantsy.ls3 {dn}
									span.fs10px-imp.clr-white.font-fantsy.ls2 DAYS
								
						
								div(class="title")
									//- p.clr-white DAYS
						div(class="col-md-1 col-md-offset-4 col-sm-1 col-sm-offset-3 text-center mt20")
							div(class="time-box days")
								
								div(class="tick")
									p.clr-white.font-fantsy.no-margin-cs7.no-padding-cs7 HOURS
									span.clr-white.fs50.font-fantsy.no-margin-cs7.no-padding-cs7 {hn}
						
						div(class="col-md-1 text-center col-sm-1 mt20")
							div(class="time-box days")
								div(class="tick")
									p.clr-white.font-fantsy.no-margin-cs7.no-padding-cs7 MINS
									span.clr-white.fs50.font-fantsy.no-margin-cs7.no-padding-cs7 {mn}
						
						div(class="col-md-1 text-center col-sm-1 mt20")
							div(class="time-box days")
								div(class="tick")
									p.clr-white.font-fantsy.no-margin-cs7.no-padding-cs7 SECS
									span.clr-white.fs50.font-fantsy.no-margin-cs7.no-padding-cs7 {sn}

