//-=== section abouts-us starts here
mixin about-us(post)

	section
		.container


				//=== section heading title paragraph start here
			.row
				.col-md-12
					.col-md-10.col-md-offset-1.col-sm-12.col-xs-12
						p.about-para.
							Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod
							tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam,
							consequat. Duis aute irure dolor in reprehenderit in voluptate velit esse
							cillum dore eu fugiat nulla pariatur. Excepteur sint occaecat cupidatat non


				//=== section heading title paragraph end here



				- name = ["lorem","lorem ","lorem"]
				
				// - post =["","founder","programer","designer"]



				//=== section team images starts here
			.row
				.col-md-6.col-md-offset-3
					.col-md-12.col-sm-12.col-xs-12
						.testimonial-sm-xs
							for i in [1,2,3]
								.col-md-4
									img(src="img/team/" + i + ".jpg").img-responsive.img-about
									h5.text-center.post-name #{post[i]}
									.mb80


			//=== section testimonial images end here


	//=== section team end here