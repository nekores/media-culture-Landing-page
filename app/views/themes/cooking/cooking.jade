mixin cooking_calculate()
	div()&attributes(attributes).cooking_calculateform
		.alpha
			.container.mb50
					.row
						.col-md-12
							+cooking-section_title("we regular lovely clients","Price", "calculate")
			.container
				.row
					.col-md-12
							form(role="form")
								.form-group
									.input-group
										.col-md-3.col-sm-3
											label.ml15
												h6 Country
												select
