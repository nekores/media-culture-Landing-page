mixin cooking_menu()
	div()&attributes(attributes).cook_menu
		.container
			.row
				.col-md-12
					+cooking-section_title("main courses . deserts . drinks","delicious", "menu")
				.owl-carousel.owl-cookingmenu
					- text1 = ["udish", "graphso","Gilyaling"]
					- text2 = ["hamburger", "meaten","Phitey"]
					- text3 = ["panney", "sharbath","davdav"]
					for i in [0,1,2,0,1]
						.col-md-12
							.cooking-imgbox1
								figure.mv20
									img(src="img/cooking/imgbox" + "/" + i + ".jpg" class="img-responsive")
									figcaption
										.layer1
										.layer2
											h3 main courses
										.layer3
											.table
												.table-middle-row
													i.fa.fa-plus
								.contentt
									.content-1.mv10.p10
										.pricebox
											h3 4$
										h4 #{text1[i]}
										hr
										p.pt5 Olives, Tomato, Greek Cheese, Oil
									.content-2.mv10.p10
										.pricebox
											h3 8$
										h4 #{text2[i]}
										hr
										p.pt5 Olives, Tomato, Greek Cheese, Oil
									.content-3.mv10.p10
										.pricebox
											h3 12$
										h4 #{text3[i]}
										hr
										p.pt5 Olives, Tomato, Greek Cheese, Oil

mixin cooking_about()
	div()&attributes(attributes).cooking_about
		.container
			.row
				.col-md-12
					+cooking-section_title("we are . ours . team","about", "cookee")
					- chefranks =[" Executive chef", "saus chef", "standard chef", "prep cook"]
					- chefname =[" Alan zengi", "Nekores singo", "shahzu sher", "navee nav"]
				.owl-carousel
					.owl-cookingmenu
						for i in [0,1,2,3]
							.team-box.col-md-12
								figure
									img(src="img/cooking/team" + "/" + i + ".png" class="img-responsive")
									figcaption
									.layer1
										h3 #{chefranks[i]}
									.layer2
										h4 #{chefname[i]}

mixin quote-widget-coooking()
	section&attributes(attributes)#widgets.quote-widget-cooking
		.alpha
			.container
				.row
					.col-md-12
						div.owl-tweeter.pv50
							div.owl-carousel(id="owl-quotes-cooking")
								- author =['Corlos lengol', 'Mr. shahzaad', 'Mrs. Shakeela', 'Mr. Nekores']
								for i in [0,1,2,3]
									div.center-block
										.col-md-12
											a(href="#")
												.img
													img.center-block.img-responsive(src='img/construction/quote'+ '/' + i + '.jpg')
										.col-md-12.col-md-12.col-xs-12
											.center-block.author-name.bg-primary.text-center.text-uppercase.bs(style="width: 200px; height:37px;")
												p.center-block.pt10.ts #{author[i]}
										.col-md-8.col-md-offset-2.col-sm-8.col-sm-offset-2.col-xs-12.text-center
											.text.mt20
												p.
													"<i>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam,
													quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo."</i>

//- PORTFOLIO SECTION CODE HERE
mixin cooking_portfolio()
	div()&attributes(attributes).cooking_portfolio
		.container
			.row
				.col-md-12
					+cooking-section_title("all images . all events","photo","gallery")
		.container-fluid#container1.no-padding
			.row.mt30
				.col-md-12.col-sm-12.col-xs-12.no-padding
					nav
						ul.nav(class="portfolio-filter-gallery" id="filters")
							li
								a.active(class="all filter active", data-filter="all") All
							li
								a(class="filter ", data-filter=".restaurants") Retaurants
							li
								a(class="filter", data-filter=".places") Places
							li
								a(class="filter", data-filter=".travels") Travels
						for i in [0,1,2,3,4,5,6,7]
							.col-md-3.mix.places.col-sm-3.no-padding
								.img-box-portfolio
									img(src="img/places/" + i + ".jpg" class="img-responsive")
									.img-content
									.font-icon
										.table-1
											.table-middle-row-1.text-center.center-block
												ul.list-inline
													li
														+icon-fa('fa-link', 'icon-frame-inverse-white icon-sm rounded-cirlce', '#')
													li.popup-gallery
														+icon-fa('fa-search-plus', ' icon-frame-inverse-white icon-sm','img/places/' + i + '.jpg')
						for i in [0,1,2,3,4,5,7,8]
							.col-md-3.mix.restaurants.travels.col-sm-3.no-padding
								.img-box-portfolio
									img(src="img/work/" + i + ".jpg" class="img-responsive")
									.img-content
									.font-icon
										.table-1
											.table-middle-row-1.text-center.center-block
												ul.list-inline
													li
														+icon-fa('fa-link', 'icon-frame-inverse-white icon-sm rounded-cirlce', '#')
													li.popup-gallery
														+icon-fa('fa-search-plus', ' icon-frame-inverse-white icon-sm','img/work/' + i + '.jpg')

