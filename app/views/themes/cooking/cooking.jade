//- CONTACT US SECTION CODE HERE
mixin cooking_contact()
	div()&attributes(attributes).cooking_contact
		.container
			.row
				.col-md-12
					+cooking-section_title("Our access . location . we are here", "cheff's", "contact")
				.col-md-12
					form(role="form")
						.col-md-4.mt40
							h6.label Your full Name*
							input(type="text" role="text" class="form-control" placeholder="Mr. Nekroes" required).p20.no-bdr-rad
						.col-md-4.mt40.mb20
							h6.label email address*
							input(type="email" role="email " class="form-control" placeholder="abc@mail.com" required).p20.no-bdr-rad
						.col-md-4.mt40.mb20
							h6.label phone number*
							input(type="text" role="text" class="form-control" placeholder="+92123456789" required).p20.no-bdr-rad
						.col-md-4.mt10
							h6.label Date*
							input(type="text" role="text " class="form-control" placeholder="Enter date" required).p20.no-bdr-rad
						.col-md-4.mt10.mb20
							h6.label time (from - to)*
							input(type="text" role="text" class="form-control" placeholder=" 12:00Pm - 12:00Am" required).p20.no-bdr-rad
						.col-md-4.mt10.mb20
							h6.label number of person*
							input(type="text" role="text " class="form-control" placeholder="Number of persons" required).p20.no-bdr-rad
						.col-md-12.mb20
							h6.label.clr-primary Description
							textarea(type="text" rows="6" class="form-control" placeholder="type your Description" required).p20.no-bdr-rad
						.col-md-6.mb30
							button.btn.btn-m-primary.btn-lg.no-bdr-rad(type="submit" value="submit") Send Message
								span.pl10.fa.fa-send


